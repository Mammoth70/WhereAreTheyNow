<?xml version="1.0" encoding="utf-8"?>
<!-- Группа кнопок FAB для YandexActivity. -->
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    >
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        style="?attr/floatingActionButtonSmallStyle"
        android:id="@+id/floatingActionButtonMapNord"
        android:clickable="true"
        android:contentDescription="@string/mapNord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:materialThemeOverlay="@style/ThemeOverlay.Material3Expressive.FloatingActionButton.Primary"
        app:srcCompat="@drawable/ic_action_compass"
        tools:ignore="ClassNameCheck"
        />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        style="?attr/floatingActionButtonSmallStyle"
        android:id="@+id/floatingActionButtonMapZoomIn"
        android:clickable="true"
        android:contentDescription="@string/mapZoomIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:materialThemeOverlay="@style/ThemeOverlay.Material3Expressive.FloatingActionButton.Primary"
        app:srcCompat="@drawable/ic_action_zoom_in"
        tools:ignore="ClassNameCheck"
        />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        style="?attr/floatingActionButtonSmallStyle"
        android:id="@+id/floatingActionButtonMapZoomOut"
        android:clickable="true"
        android:contentDescription="@string/mapZoomOut"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="128dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:materialThemeOverlay="@style/ThemeOverlay.Material3Expressive.FloatingActionButton.Primary"
        app:srcCompat="@drawable/ic_action_zoom_out"
        tools:ignore="ClassNameCheck"
        />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        style="?attr/floatingActionButtonSmallStyle"
        android:id="@+id/floatingActionButtonMapTilt"
        android:clickable="true"
        android:contentDescription="@string/map2d"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:materialThemeOverlay="@style/ThemeOverlay.Material3Expressive.FloatingActionButton.Primary"
        app:srcCompat="@drawable/ic_action_2d"
        tools:ignore="ClassNameCheck"
        />
</merge>